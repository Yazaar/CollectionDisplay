<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\imageA.png" />
    <None Remove="Images\imageB.png" />
    <None Remove="Images\imageC.png" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CollectionDisplay\CollectionDisplay.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\imageA.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\imageB.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\imageC.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
